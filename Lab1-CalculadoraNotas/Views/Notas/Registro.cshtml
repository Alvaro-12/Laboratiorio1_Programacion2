@model List<Lab1_CalculadoraNotas.Models.TblNotasEstudiante>
@{
    ViewBag.Title = "Resultado";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Resultado</h2>
<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Periodo1</th>
            <th>Periodo2</th>
            <th>Periodo3</th>
            <th>Total Final Periodo :</th>

        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>

                <th>@item.Id</th>
                <th>@item.Nombre</th>
                <th>@item.Periodo1</th>
                <th>@item.Periodo2</th>
                <th>@item.Periodo3</th>
                <th>@item.NotaFianl</th>

            </tr>
         }

    </tbody>
</table>
